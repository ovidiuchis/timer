{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    }
  },
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/ec440b30-1b13-411f-bd2d-33255e3ed324/v2.0",
          "clientIdSettingName": "AAD_CLIENT_ID"
        }
      }
    }
  }
}
